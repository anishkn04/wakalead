{
  "name": "wakatime-leaderboard",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=wakalead --commit-dirty=true",
    "deploy:both": "npm run build && wrangler pages deploy dist --project-name=wakalead --commit-dirty=true && npx wrangler deploy",
    "worker:dev": "wrangler dev",
    "worker:deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 execute wakalead --file=./schema.sql"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "chart.js": "^4.4.7",
    "react-chartjs-2": "^5.2.0",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250106.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "wrangler": "^3.103.2"
  }
}
